{
    "name": "@nomadmystic/github-dependencies",
    "version": "0.0.2",
    "description": "This React App will show, update, commit, and push repositories to GitHub",
    "type": "module",
    "scripts": {
        "start": "node index.js",
        "dev": "npm run development",
        "development": "concurrently \"NODE_ENV=development npm run webpack:watch\" \"nodemon -q ../../build/server/index.js\"",
        "prod": "npm run production",
        "production": "tsc -p tsconfig.json",
        "postpack": "tsc --project ./"
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:nomad-mystic/github-dependencies.git"
    },
    "bugs": {
        "url": "https://github.com/nomad-mystic/github-dependencies/issues"
    },
    "homepage": "https://github.com/nomad-mystic/github-dependencies#readme",
    "keywords": [
        "GitHub",
        "React",
        "Dependencies Management"
    ],
    "author": "Keith Murphy",
    "license": "ISC",
    "dependencies": {
        "@octokit/core": "^5.0.0",
        "@octokit/plugin-rest-endpoint-methods": "^9.0.0",
        "@octokit/request": "^8.1.1",
        "@octokit/types": "^11.1.0",
        "@types/express": "^4.17.17",
        "@types/morgan": "^1.9.5",
        "@types/node": "^20.5.9",
        "express": "^4.18.2",
        "typescript": "^5.2.2",
        "ts-loader": "^9.4.4",
        "morgan": "^1.10.0"
    },
    "devDependencies": {
        "concurrently": "^8.2.1",
        "nodemon": "^3.0.1"
    }
}
