{
  "name": "github-dependencies",
  "version": "0.0.1",
  "description": "This React App will show, update, commit, and push repositories to GitHub",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "nodemon ./dist/server.js",
    "dev": "concurrently \"tsc --watch --project ./\" \"nodemon -q ./dist/server.js\"",
    "development": "tsc --watch --project ./",
    "prod": "npm run production",
    "production:build": "tsc --project ./",
    "postpack": "tsc --project ./"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:nomad-mystic/github-dependencies.git"
  },
  "keywords": [
    "GitHub",
    "React",
    "Dependencies Management"
  ],
  "author": "Keith Murphy",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.17.17",
    "@types/morgan": "^1.9.5",
    "@types/node": "^20.5.9",
    "express": "^4.18.2",
    "react": "^18.2.0",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "concurrently": "^8.2.1",
    "morgan": "^1.10.0",
    "nodemon": "^3.0.1"
  }
}
