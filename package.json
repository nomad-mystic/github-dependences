{
  "name": "github-dependencies",
  "version": "0.0.1",
  "description": "This React App will show, update, commit, and push repositories to GitHub",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"tsc --watch --project ./\" \"nodemon -q build/server/index.js\" \"npm run webpack\"",
    "development": "tsc --watch --project ./",
    "webpack": "webpack -w --config=webpack.config.js",
    "prod": "npm run production",
    "production:build": "tsc --project ./",
    "postpack": "tsc --project ./"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:nomad-mystic/github-dependencies.git"
  },
  "keywords": [
    "GitHub",
    "React",
    "Dependencies Management"
  ],
  "author": "Keith Murphy",
  "license": "ISC",
  "dependencies": {
    "@octokit/core": "^5.0.0",
    "@octokit/plugin-rest-endpoint-methods": "^9.0.0",
    "@octokit/request": "^8.1.1",
    "@types/express": "^4.17.17",
    "@types/morgan": "^1.9.5",
    "@types/node": "^20.5.9",
    "express": "^4.18.2",
    "octokit": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "concurrently": "^8.2.1",
    "morgan": "^1.10.0",
    "nodemon": "^3.0.1",
    "ts-loader": "^9.4.4",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  }
}
